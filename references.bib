  file = {/Users/eason01/Zotero/storage/4WGZAM2E/Zhang et al. - 2016 - G9a-mediated methylation of ERα links the PHF20MO.pdf}
}

@article{zhangPHF20RegulatesNFkB2013,
  title = {{{PHF20}} Regulates {{NF-κB}} Signalling by Disrupting Recruitment of {{PP2A}} to P65},
  author = {Zhang, Tiejun and Ah Park, Kyeong and Li, Yuwen and Sun Byun, Hee and Jeon, Juhee and Lee, Yoonjung and Hee Hong, Jang and Man Kim, Jin and Huang, Song-Mei and Choi, Seung-Won and Kim, Seon-Hwan and Sohn, Kyung-Cheol and Ro, Hyunju and Hoon Lee, Ji and Lu, Tao and Stark, George R. and Shen, Han-Ming and Liu, Zheng-gang and Park, Jongsun and Min Hur, Gang},
  date = {2013-06-25},
