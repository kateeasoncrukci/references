@article{molitorMolecularGeneticAnalysis2013,
  title = {Molecular Genetic Analysis of {{VRK1}} in Mammary Epithelial Cells: Depletion Slows Proliferation in Vitro and Tumor Growth and Metastasis in Vivo},
  shorttitle = {Molecular Genetic Analysis of {{VRK1}} in Mammary Epithelial Cells},
  author = {Molitor, T. P. and Traktman, P.},
