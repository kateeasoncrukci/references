@article{zhangG9amediatedMethylationERa2016,
  title = {G9a-Mediated Methylation of {{ERα}} Links the {{PHF20}}/{{MOF}} Histone Acetyltransferase Complex to Hormonal Gene Expression},
  author = {Zhang, Xi and Peng, Danni and Xi, Yuanxin and Yuan, Chao and Sagum, Cari A. and Klein, Brianna J. and Tanaka, Kaori and Wen, Hong and Kutateladze, Tatiana G. and Li, Wei and Bedford, Mark T. and Shi, Xiaobing},
  date = {2016-03-10},
