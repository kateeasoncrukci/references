@article{fontanaContextMattersConsensus2019,
  title = {Context Matters—Consensus Molecular Subtypes of Colorectal Cancer as Biomarkers for Clinical Trials},
  author = {Fontana, E and Eason, K and Cervantes, A and Salazar, R and Sadanandam, A},
