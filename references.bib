@article{shenARID1ADeficiencyImpairs2015,
  title = {{{ARID1A Deficiency Impairs}} the {{DNA Damage Checkpoint}} and {{Sensitizes Cells}} to {{PARP Inhibitors}}},
  author = {Shen, Jianfeng and Peng, Yang and Wei, Leizhen and Zhang, Wei and Yang, Lin and Lan, Li and Kapoor, Prabodh and Ju, Zhenlin and Mo, Qianxing and Shih, Ie-Ming and Uray, Ivan P. and Wu, Xiangwei and Brown, Powel H. and Shen, Xuetong and Mills, Gordon B. and Peng, Guang},
  date = {2015-07-06},
  journaltitle = {Cancer Discovery},
  shortjournal = {Cancer Discovery},
