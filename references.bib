@article{insaPrognosticFactorsPredicting1999,
  title = {Prognostic Factors Predicting Survival from First Recurrence in Patients with Metastatic Breast Cancer: Analysis of 439 Patients},
  shorttitle = {Prognostic Factors Predicting Survival from First Recurrence in Patients with Metastatic Breast Cancer},
  author = {Insa, Amelia and Lluch, Ana and Prosper, Felipe and Marugan, Isabel and Martinez‐Agullo, Angel and Garcia‐Conde, Javier},
  date = {1999-07-01},
  journaltitle = {Breast Cancer Research and Treatment},
